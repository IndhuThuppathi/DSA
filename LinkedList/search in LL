code:
class Solution {
  public:
    // Function to count nodes of a linked list.
    bool searchKey(int n, Node* head, int key) {
        // Code here
        Node* current=head;
        while(current!=NULL)
        {
            if(current->data==key)
            {
                return true;
                break;
            }
            current=current->next;
        }
        if(current==NULL){
        return false;}
        return head;
    }
    
};
